[ignore]
<PROJECT_ROOT>/build/.*
<PROJECT_ROOT>/config/.*
<PROJECT_ROOT>/dist/.*
<PROJECT_ROOT>/node_modules/.*
<PROJECT_ROOT>/static/.*
<PROJECT_ROOT>/test/.*
<PROJECT_ROOT>/src/assets/.*
[include]

[libs]
flow-typed

[options]
include_warnings=true
module.file_ext=.vue
module.file_ext=.js
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
suppress_comment=\\(.\\|\n\\)*\\$FlowIgnore
module.name_mapper='^@\/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
module.name_mapper='^ACE_BUILDS\/\(.*\)$' -> '<PROJECT_ROOT>/static/ace-builds/\1'

